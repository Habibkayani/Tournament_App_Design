<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tournament">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TOURNAMENT">
        <activity android:name="Act.Organizer_Tournament_Detail"></activity>
        <activity android:name="Act.Organizer_Create_New_Tournament" />
        <activity android:name="Act.Search_User" />
        <activity android:name="Act.Catagory_Name" />
        <activity android:name="Act.Notification" />
        <activity android:name="Act.OrganizerDashBoard" />
        <activity android:name="Act.MyTeams" />
        <activity android:name="Act.TeamDetail" />
        <activity android:name="Act.Tournament_Detials" />
        <activity
            android:name="Act.Create_New_Tournament"
            android:label="@string/title_activity_create__new__tournament"
            android:theme="@style/Theme.TOURNAMENT.NoActionBar" />
        <activity android:name="Act.TournamentCatagories" />
        <activity android:name="Act.UserDashboard" />
        <activity android:name="Act.ForUserSignUp" />
        <activity android:name="Act.LoginAct" />
        <activity android:name="Act.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="Act.MainActivity" />
    </application>

</manifest>