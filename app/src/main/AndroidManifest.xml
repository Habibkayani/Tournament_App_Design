<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tournament">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TOURNAMENT">
        <activity android:name="Act.TournamentCatagories"></activity>
        <activity android:name="Act.UserDashboard" />
        <activity android:name="Act.ForUserSignUp" />
        <activity android:name="Act.LoginAct" />
        <activity android:name="Act.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="Act.MainActivity" />
    </application>

</manifest>